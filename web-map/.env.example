# Variables d'environnement pour web-map

# URL de l'API d'authentification
REACT_APP_API_AUTH_URL=http://localhost:3000/api/auth

# URL de l'API des signalements
REACT_APP_API_SIGNALEMENTS_URL=http://localhost:3000/api

# URL du serveur de tuiles (cartes offline)
# Option 1: OSM en ligne (par défaut)
REACT_APP_TILE_SERVER_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png

# Option 2: Serveur de tuiles offline local
# REACT_APP_TILE_SERVER_URL=http://localhost:8080/tile/{z}/{x}/{y}.png

# Port de l'application React en développement
PORT=3001
